{
	"name": "tellraw-preview",
	"displayName": "/tellraw preview",
	"description": "Preview /tellraw messages",
	"publisher": "actuallyboomber",
	"license": "MIT",
	"version": "0.2.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"minecraft",
		"command",
		"chat"
	],
	"activationEvents": [
		"onLanguage:mcfunction"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "tellraw_preview.previewColor",
				"description": "Color of the preview message",
				"defaults": {
					"dark": "#dbdbdb60",
					"light": "#00000094",
					"highContrast": "#FFFFFFFF"
				}
			},
			{
				"id": "tellraw_preview.errorColor",
				"description": "Color of the error message",
				"defaults": {
					"dark": "#d62424a9",
					"light": "#dd3636c2",
					"highContrast": "#d30000d5"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
